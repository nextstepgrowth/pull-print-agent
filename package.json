{
  "name": "pull-print-agent",
  "version": "1.0.0",
  "description": "Pull print agent Electron + React + Vite app",
  "main": "dist/electron/main.js",
  "build": {
    "files": [
      "dist/electron/**",
      "dist/renderer/**",
      "dist/src/**",
      "node_modules/**",
      "package.json"
    ]
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "build": "tsc && vite build",
    "start": "node dist/src/server.js",
    "typecheck": "tsc --noEmit",
    "electron": "electron dist/electron/main.js",
    "vite": "vite",
    "dev:electron": "concurrently -k \"npm:dev\" \"vite\" \"electron dist/electron/main.js\"",
    "dist": "npm run build && electron-builder",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^7.0.1",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "express": "^5.1.0",
    "form-data": "^4.0.3",
    "lucide-react": "^0.525.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/archiver": "^6.0.3",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.13",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "electron": "^37.2.1",
    "electron-builder": "^24.13.2",
    "eslint": "^9.30.1",
    "jest": "^30.0.4",
    "multer": "^2.0.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "supertest": "^7.1.3",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "vite": "^4.5.0",
    "vite-plugin-electron": "^0.29.0"
  }
}
